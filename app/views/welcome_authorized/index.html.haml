= form_tag welcome_authorized_index_path, :method => 'get' do
	%p
		= text_field_tag :search, params[:search]
		= submit_tag "Search", :name => nil
%table
	%tbody
		%tr	
			- if @events 
				- @events.each do |event|
					%td
						= image_tag event.image.url(:medium)
						%p
						= event.title
						= event.location
